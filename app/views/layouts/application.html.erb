<!DOCTYPE html>
<html>
<head>
  <title>THE BLHACKATHON</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: name %>
  <% end %>
  <%= render 'layouts/nav' %>
  <div class="l-container">
    <%= render 'info/about' %>
    <%= yield %>
  </div>
  <%= render 'layouts/footer' %>
</body>
</html>
